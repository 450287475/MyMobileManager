<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:save_setting_layout="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.mumuseng.mymobilemanager.SaveSetting3Activity">
    <TextView
       style="@style/tv_title"
        android:text="3.设置安全号码"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:text="sim卡变更后\r\n报警短信会发送给安全号码"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/et_saveset3_contact"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="选择联系人"
        android:id="@+id/bt_saveset3_chooseContact"
        android:background="@drawable/btn_green_normal"/>
    <com.example.mumuseng.view.SaveSettingLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        save_setting_layout:item="3"
        ></com.example.mumuseng.view.SaveSettingLayout>
</LinearLayout>
